!!!
%html
  %head
    %title #{yield :title}
    = stylesheet_link_tag 'application', :media => 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    = render 'layouts/nav'
    .container
      - if notice
        .notice #{notice}
      - if alert
        .alert #{alert}
      = yield